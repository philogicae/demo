  "scripts": {
    "migrate-metadata": "node scripts/migrate_metadata.js",
    "dev": "next dev -H 0.0.0.0 --port 3000 --turbopack",
    "build": "pnpm run migrate-metadata && pnpm run lint && next build",
    "start": "next start",
    "serve": "serve out",
    "lint": "biome check --write app",
    "upgrade": "pnpm install next@v15.0.2-canary.4 react@rc react-dom@rc @biomejs/biome@latest && biome migrate --write && pnpm update && pnpm prune",
    "clean": "sudo rm -rv node_modules | pv -l -s $(find node_modules | wc -l) > /dev/null",
    "solhint": "solhint contracts/**/*.sol",
    "flatten": "mkdir -p artifacts && hardhat flatten > artifacts/flatten.sol",
    "compile": "hardhat compile",
    "deploy-test": "hardhat run scripts/deploy.js --network sepolia",
    "verify-test": "hardhat verify --network sepolia",
    "deploy-prod": "hardhat run scripts/deploy.js --network avalanche",
    "verify-prod": "hardhat verify --network avalanche"
  },
  "dependencies": {
    "@akamfoad/qrcode": "^0.3.2",
