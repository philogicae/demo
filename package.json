    "react-dom": "19.0.0-rc-1631855f-20241023",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.27.0",
    "viem": "^2.21.34",
    "wagmi": "^2.12.25"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@gelatonetwork/relay-context": "^3.2.0",
